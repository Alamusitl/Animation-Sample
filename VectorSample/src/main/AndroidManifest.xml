<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.owl.vectorsample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".PreLActivity"
            android:label="Example Pre L"/>
        <activity
            android:name=".LActivity"
            android:label="Example L"/>
        <activity
            android:name=".SmileActivity"
            android:label="@string/smiling_face"/>
        <activity
            android:name=".ClockActivity"
            android:label="@string/clock"/>
        <activity
            android:name=".FillInHeartActivity"
            android:label="@string/fill_in_heart"/>
        <activity
            android:name=".VectorActivity"
            android:label="Simpson"/>
        <activity
            android:name=".GiftActivity"
            android:label="Gift"/>
        <activity
            android:name=".SearchIconActivity"
            android:label="Search Icon"/>
        <activity
            android:name=".SearchBarActivity"
            android:label="SearchBar"/>
        <activity
            android:name=".StarActivity"
            android:label="Star"/>
        <activity
            android:name=".ArrowActivity"
            android:label="Arrow"/>
        <activity
            android:name=".ColorGradientActivity"
            android:label="Color gradient"/>
    </application>

</manifest>